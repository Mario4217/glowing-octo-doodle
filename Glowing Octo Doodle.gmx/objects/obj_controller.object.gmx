            <string>///networking code
switch (async_load[? "type"]){
  case network_type_non_blocking_connect:
    if (async_load[? "succeeded"]){
      nw_is_connected = true;
      is_server = false;
      console_print("Connected to server!")
      nw_buffer = buffer_create(1, buffer_grow, 1);
    }else{
      nw_is_connected = false;
      is_server = true;
      console_print("Connection to server failer!",c_orange);
    }
  break;
  case network_type_connect:
    if (is_server){
      var player = nw_make_player(async_load[? "id"]);
      nw_players[? async_load[? "id"]] = player;
      console_print("Someone connected to the server!");
      buffer_seek(nw_buffer, buffer_seek_start, 0);
      buffer_write(nw_buffer, buffer_u8, NW.server_welcome);
      buffer_write(nw_buffer, buffer_string, GM_version);
      network_send_packet(async_load[? "id"], nw_buffer, buffer_get_size(nw_buffer));
    }
  break;
  case network_type_data:
    if (is_server){
      nw_income_server(async_load[? "buffer"]);
    }else{
      nw_income_client(async_load[? "buffer"]);
    }
  break;
  case network_type_disconnect:
    var map = nw_players[? async_load[? "id"]];
    with (map[? "pawn"]){
      instance_destroy();
    }
    ds_map_delete(nw_players, async_load[? "id"]);
    console_print("Someone disconnected from the server!")
  break;
}
