    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>var xmov = bind_check("player_horizontal");
var ymov = bind_check("player_vertical");

//player_pawn.dir = 3;
player_pawn.walking = false;

if (xmov &gt; 0){
  player_pawn.dir = 0;
  player_pawn.walking = true;
}else if (xmov &lt; 0){
  player_pawn.dir = 2;
  player_pawn.walking = true;
}else if (ymov &gt; 0){
  player_pawn.dir = 3;
  player_pawn.walking = true;
}else if (ymov &lt; 0){
  player_pawn.dir = 1;
  player_pawn.walking = true;
}

var dir = point_direction(0, 0, xmov, ymov);

var spd = game_delta_time * move_spd * point_distance(0, 0, xmov, ymov);

if (!collision_rectangle(player_pawn.x-16+xmov*spd, player_pawn.y-16, player_pawn.x+16+xmov*spd, player_pawn.y, obj_clip, 0, 0)){
  player_pawn.x += xmov*spd;
}
if (!collision_rectangle(player_pawn.x-16, player_pawn.y-16+ymov*spd, player_pawn.x+16, player_pawn.y+ymov*spd, obj_clip, 0, 0)){
  player_pawn.y += ymov*spd;
}
/*
if (point_distance(0, 0, xmov*spd, ymov*spd) &gt; 0) {
    move(spd, dir, player_pawn);
}
*/

view_xview[0] = floor(player_pawn.x - view_wview[0]/2);
view_yview[0] = floor(player_pawn.y - view_hview[0]/2);

if (pickup != -1){
  if (bind_check_pressed('putdown') &amp;&amp; point_distance(player_pawn.x, player_pawn.y, mouse_x, mouse_y) &lt; pickup_distance
  &amp;&amp; !collision_rectangle(mouse_x-pickup.sprite_xoffset, mouse_y-pickup.sprite_yoffset, mouse_x-pickup.sprite_xoffset+pickup.sprite_width, mouse_y-pickup.sprite_yoffset+pickup.sprite_height, obj_clip, 0, 1)
  &amp;&amp; !collision_line(mouse_x, mouse_y, player_pawn.x, player_pawn.y, obj_clip, 0, 1)
  ){
    pickup.x = mouse_x;
    pickup.y = mouse_y;
    pickup.visible = true;
    if (gameplay_object_have_tag(pickup.src_map[? "id"],"clip")){
      pickup.clip.x = pickup.x - pickup.sprite_xoffset;
      pickup.clip.y = pickup.y - pickup.sprite_yoffset;
    }
    pickup = -1;
  }
}
player_pawn.pickup = pickup;

</string>
          </argument>
        </arguments>
      </action>
